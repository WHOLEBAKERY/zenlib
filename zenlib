		-- ========== CREATE DROPDOWN ==========
		function Content.CreateDropdown(DropdownTitle, list, callback)
			callback = callback or function() end
			DropdownTitle = DropdownTitle or "Dropdown"
			list = list or {}
			local IsDropped = false
			local DropYSize = 145
			local NormalYSize = 38
			
			local DropdownFrame = Instance.new("Frame")
			local DropdownCorner = Instance.new("UICorner")
			local DropdownStroke = Instance.new("UIStroke")
			local DropdownLabel = Instance.new("TextLabel")		
			local DropdownTrigger = Instance.new("TextButton")
			local DropArrow = Instance.new("TextLabel")
			local DropdownContainer = Instance.new("ScrollingFrame")
			local ContentLayout = Instance.new("UIListLayout")
			local ContentPadding = Instance.new("UIPadding")

			DropdownFrame.Name = "DropdownFrame"
			DropdownFrame.Parent = TabContent
			DropdownFrame.Active = true
			DropdownFrame.BackgroundColor3 = Color3.fromRGB(20, 35, 55)
			DropdownFrame.BackgroundTransparency = 0.2
			DropdownFrame.BorderColor3 = Color3.fromRGB(0, 0, 0)
			DropdownFrame.BorderSizePixel = 0
			DropdownFrame.ClipsDescendants = true
			DropdownFrame.Position = UDim2.new(0.0983050838, 0, 0.465454549, 0)
			DropdownFrame.Selectable = true
			DropdownFrame.Size = UDim2.new(0, 267, 0, NormalYSize)
			DropdownFrame.ZIndex = 4

			DropdownCorner.CornerRadius = UDim.new(0, 10)
			DropdownCorner.Name = "DropdownCorner"
			DropdownCorner.Parent = DropdownFrame

			DropdownStroke.Name = "DropdownStroke"
			DropdownStroke.Parent = DropdownFrame
			DropdownStroke.Color = Color3.fromRGB(50, 130, 255)
			DropdownStroke.Thickness = 1
			DropdownStroke.Transparency = 0.6

			DropdownLabel.Name = "DropdownLabel"
			DropdownLabel.Parent = DropdownFrame
			DropdownLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			DropdownLabel.BackgroundTransparency = 1.000
			DropdownLabel.BorderColor3 = Color3.fromRGB(0, 0, 0)
			DropdownLabel.BorderSizePixel = 0
			DropdownLabel.Position = UDim2.new(0.05, 0, 0, 0)
			DropdownLabel.Size = UDim2.new(0, 200, 0, 38)
			DropdownLabel.ZIndex = 5
			DropdownLabel.Font = Enum.Font.GothamMedium
			DropdownLabel.Text = DropdownTitle
			DropdownLabel.TextColor3 = Color3.fromRGB(220, 235, 255)
			DropdownLabel.TextSize = 16.000
			DropdownLabel.TextWrapped = true
			DropdownLabel.TextXAlignment = Enum.TextXAlignment.Left
	
			-- Dropdown arrow indicator
			DropArrow.Name = "DropArrow"
			DropArrow.Parent = DropdownFrame
			DropArrow.BackgroundTransparency = 1
			DropArrow.Position = UDim2.new(0.88, 0, 0.2, 0)
			DropArrow.Size = UDim2.new(0, 20, 0, 20)
			DropArrow.ZIndex = 5
			DropArrow.Font = Enum.Font.GothamBold
			DropArrow.Text = "▼"
			DropArrow.TextColor3 = Color3.fromRGB(100, 180, 255)
			DropArrow.TextSize = 12
			
			DropdownTrigger.Name = "DropdownTrigger"
			DropdownTrigger.Parent = DropdownFrame
			DropdownTrigger.BackgroundColor3 = Color3.fromRGB(53, 50, 74)
			DropdownTrigger.BackgroundTransparency = 1.000
			DropdownTrigger.BorderColor3 = Color3.fromRGB(0, 0, 0)
			DropdownTrigger.BorderSizePixel = 0
			DropdownTrigger.ClipsDescendants = true
			DropdownTrigger.Size = UDim2.new(0, 267, 0, 38)
			DropdownTrigger.ZIndex = 5
			DropdownTrigger.Text = ""
			DropdownTrigger.TextTransparency = 1.000
			
			DropdownTrigger.MouseButton1Click:Connect(function()
				if IsDropped == false then 
					local ClickSound = Instance.new("Sound")
					ClickSound.Parent = UiLib
					ClickSound.SoundId = "rbxassetid://552900451"
					ClickSound.Volume = 2 
					ClickSound.Pitch = 1.2
					
					game.TweenService:Create(DropArrow, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
						Rotation = 180
					}):Play()
					
					DropdownFrame.Size = UDim2.new(0, 267, 0, DropYSize)
					DropdownContainer.Visible = true
					ClickSound:Play()
					IsDropped = true
				else
					local ClickSound = Instance.new("Sound")
					ClickSound.Parent = UiLib
					ClickSound.SoundId = "rbxassetid://552900451"
					ClickSound.Volume = 2 
					ClickSound.Pitch = 1

					game.TweenService:Create(DropArrow, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
						Rotation = 0
					}):Play()

					DropdownFrame.Size = UDim2.new(0, 267, 0, NormalYSize)
					DropdownContainer.Visible = false
					ClickSound:Play()
					IsDropped = false
				end
			end)

			DropdownContainer.Parent = DropdownFrame
			DropdownContainer.Active = true
			DropdownContainer.BackgroundColor3 = Color3.fromRGB(53, 50, 74)
			DropdownContainer.BorderColor3 = Color3.fromRGB(0, 0, 0)
			DropdownContainer.BackgroundTransparency = 1
			DropdownContainer.BorderSizePixel = 0
			DropdownContainer.Position = UDim2.new(0.06, 0, 0.35, 0)
			DropdownContainer.Size = UDim2.new(0, 237, 0, 85)
			DropdownContainer.ZIndex = 5
			DropdownContainer.CanvasSize = UDim2.new(0, 0, 1, 0)
			DropdownContainer.Visible = false
			DropdownContainer.ScrollBarThickness = 3
			DropdownContainer.ScrollBarImageColor3 = Color3.fromRGB(50, 130, 255)

			ContentLayout.Name = "ContentLayout"
			ContentLayout.Parent = DropdownContainer
			ContentLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
			ContentLayout.SortOrder = Enum.SortOrder.LayoutOrder
			ContentLayout.Padding = UDim.new(0, 6)
			
			for i,v in next, list do
				local Option = Instance.new("TextButton")
				local UICorner = Instance.new("UICorner")
				
				Option.Name = "Option"
				Option.Parent = DropdownContainer
				Option.BackgroundColor3 = Color3.fromRGB(30, 50, 80) -- Dark blue options
				Option.BackgroundTransparency = 0.3
				Option.BorderColor3 = Color3.fromRGB(0, 0, 0)
				Option.BorderSizePixel = 0
				Option.Size = UDim2.new(0, 220, 0, 26)
				Option.Font = Enum.Font.Gotham
				Option.Text = v
				Option.TextColor3 = Color3.fromRGB(200, 220, 255)
				Option.TextSize = 14.000
				Option.MouseButton1Click:Connect(function()
					local ClickSound = Instance.new("Sound")
					ClickSound.Parent = UiLib
					ClickSound.SoundId = "rbxassetid://552900451"
					ClickSound.Volume = 2 
					ClickSound.Pitch = 1

					callback(v)
					ClickSound:Play()
					
					game.TweenService:Create(DropArrow, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
						Rotation = 0
					}):Play()
					
					DropdownFrame.Size = UDim2.new(0, 267, 0, NormalYSize)
					DropdownContainer.Visible = false		
					IsDropped = false

					wait(0.5)
					ClickSound:Destroy()
				end)

				-- Hover effect for options
				Option.MouseEnter:Connect(function()
					game.TweenService:Create(Option, TweenInfo.new(0.2, Enum.EasingStyle.Quad), {
						BackgroundTransparency = 0.1
					}):Play()
				end)
				
				Option.MouseLeave:Connect(function()
					game.TweenService:Create(Option, TweenInfo.new(0.2, Enum.EasingStyle.Quad), {
						BackgroundTransparency = 0.3
					}):Play()
				end)

				UICorner.CornerRadius = UDim.new(0, 6)
				UICorner.Parent = Option
			end

			ContentPadding.Name = "ContentPadding"
			ContentPadding.Parent = DropdownContainer
			ContentPadding.PaddingTop = UDim.new(0, 8)
		end
		
		-- ========== CREATE LABEL ==========
		function Content.CreateLabel(LabelTitle)
			local LabelFrame = Instance.new("Frame")
			local LabelCorner = Instance.new("UICorner")
			local LabelStroke = Instance.new("UIStroke")
			local LabelText = Instance.new("TextLabel")
			local LabelGradient = Instance.new("UIGradient")
			
			LabelFrame.Name = "LabelFrame"
			LabelFrame.Parent = TabContent
			LabelFrame.Active = true
			LabelFrame.BackgroundColor3 = Color3.fromRGB(25, 45, 70) -- Slightly lighter blue
			LabelFrame.BackgroundTransparency = 0.3
			LabelFrame.BorderColor3 = Color3.fromRGB(0, 0, 0)
			LabelFrame.BorderSizePixel = 0
			LabelFrame.ClipsDescendants = true
			LabelFrame.Position = UDim2.new(0.0983050838, 0, 0.043636363, 0)
			LabelFrame.Selectable = true
			LabelFrame.Size = UDim2.new(0, 267, 0, 35)
			LabelFrame.ZIndex = 4

			LabelCorner.CornerRadius = UDim.new(0, 10)
			LabelCorner.Name = "LabelCorner"
			LabelCorner.Parent = LabelFrame

			LabelStroke.Name = "LabelStroke"
			LabelStroke.Parent = LabelFrame
			LabelStroke.Color = Color3.fromRGB(80, 160, 255) -- Brighter blue
			LabelStroke.Thickness = 1.2
			LabelStroke.Transparency = 0.5

			LabelText.Name = "LabelText"
			LabelText.Parent = LabelFrame
			LabelText.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			LabelText.BackgroundTransparency = 1.000
			LabelText.BorderColor3 = Color3.fromRGB(0, 0, 0)
			LabelText.BorderSizePixel = 0
			LabelText.Position = UDim2.new(0.05, 0, 0, 0)
			LabelText.Size = UDim2.new(0, 250, 0, 35)
			LabelText.ZIndex = 5
			LabelText.Font = Enum.Font.GothamBold
			LabelText.Text = LabelTitle
			LabelText.TextColor3 = Color3.fromRGB(255, 255, 255)
			LabelText.TextSize = 17.000
			LabelText.TextWrapped = true
			LabelText.TextScaled = true
			LabelText.TextXAlignment = Enum.TextXAlignment.Center

			-- Blue gradient effect
			LabelGradient.Name = "LabelGradient"
			LabelGradient.Parent = LabelText
			LabelGradient.Color = ColorSequence.new{
				ColorSequenceKeypoint.new(0, Color3.fromRGB(120, 200, 255)),
				ColorSequenceKeypoint.new(1, Color3.fromRGB(60, 140, 255))
			}
		end
		
		-- ========== CREATE DIVIDER ==========
		function Content.CreateDivider(DividerTitle)
			local DividerFrame = Instance.new("Frame")
			local DividerCorner = Instance.new("UICorner")
			local DividerText = Instance.new("TextLabel")
			local LeftLine = Instance.new("Frame")
			local RightLine = Instance.new("Frame")
			local LeftCorner = Instance.new("UICorner")
			local RightCorner = Instance.new("UICorner")

			DividerFrame.Name = "DividerFrame"
			DividerFrame.Parent = TabContent
			DividerFrame.Active = true
			DividerFrame.BackgroundColor3 = Color3.fromRGB(20, 35, 55)
			DividerFrame.BackgroundTransparency = 0.8 -- Very transparent
			DividerFrame.BorderColor3 = Color3.fromRGB(0, 0, 0)
			DividerFrame.BorderSizePixel = 0
			DividerFrame.ClipsDescendants = false
			DividerFrame.Position = UDim2.new(0.0983050838, 0, 0.043636363, 0)
			DividerFrame.Selectable = true
			DividerFrame.Size = UDim2.new(0, 267, 0, 25)
			DividerFrame.ZIndex = 4

			DividerCorner.CornerRadius = UDim.new(0, 8)
			DividerCorner.Name = "DividerCorner"
			DividerCorner.Parent = DividerFrame

			DividerText.Name = "DividerText"
			DividerText.Parent = DividerFrame
			DividerText.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			DividerText.BackgroundTransparency = 1.000
			DividerText.BorderColor3 = Color3.fromRGB(0, 0, 0)
			DividerText.BorderSizePixel = 0
			DividerText.Position = UDim2.new(0.3, 0, 0, 0)
			DividerText.Size = UDim2.new(0, 110, 0, 25)
			DividerText.ZIndex = 5
			DividerText.Font = Enum.Font.GothamBold
			DividerText.Text = DividerTitle
			DividerText.TextColor3 = Color3.fromRGB(150, 190, 230) -- Soft blue
			DividerText.TextSize = 14.000
			DividerText.TextWrapped = true

			-- Left decorative line
			LeftLine.Name = "LeftLine"
			LeftLine.Parent = DividerFrame
			LeftLine.BackgroundColor3 = Color3.fromRGB(50, 130, 255)
			LeftLine.BackgroundTransparency = 0.5
			LeftLine.BorderSizePixel = 0
			LeftLine.Position = UDim2.new(0.05, 0, 0.45, 0)
			LeftLine.Size = UDim2.new(0, 60, 0, 2)
			LeftLine.ZIndex = 4

			LeftCorner.CornerRadius = UDim.new(1, 0)
			LeftCorner.Parent = LeftLine

			-- Right decorative line
			RightLine.Name = "RightLine"
			RightLine.Parent = DividerFrame
			RightLine.BackgroundColor3 = Color3.fromRGB(50, 130, 255)
			RightLine.BackgroundTransparency = 0.5
			RightLine.BorderSizePixel = 0
			RightLine.Position = UDim2.new(0.72, 0, 0.45, 0)
			RightLine.Size = UDim2.new(0, 60, 0, 2)
			RightLine.ZIndex = 4

			RightCorner.CornerRadius = UDim.new(1, 0)
			RightCorner.Parent = RightLine
		end
		
		return Content
	end
	return TabSys
end
return Lib
