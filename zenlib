	ContentHolder.Parent = Main
	
	Lib:Drag(TopBar, Main)
	Lib:Drag(MinimizerBox, MinimizerBox)
	
	-- ========== MINIMIZE/MAXIMIZE LOGIC ==========
	local isMinimized = false
	
	local function toggleMinimize()
		local ClickSound = Instance.new("Sound")
		ClickSound.Parent = UiLib
		ClickSound.SoundId = "rbxassetid://552900451"
		ClickSound.Volume = 2
		
		if not isMinimized then
			-- Minimize
			ClickSound.Pitch = 0.8
			Main.Visible = false
			MinimizerBox.Visible = true
			isMinimized = true
		else
			-- Maximize
			ClickSound.Pitch = 1
			Main.Visible = true
			MinimizerBox.Visible = false
			isMinimized = false
		end
		
		ClickSound:Play()
		wait(0.5)
		ClickSound:Destroy()
	end
	
	MinimizeButton.MouseButton1Click:Connect(toggleMinimize)
	MinimizerButton.MouseButton1Click:Connect(toggleMinimize)
	
	-- Hover effects for minimize button
	MinimizeButton.MouseEnter:Connect(function()
		tweenService:Create(MinimizeButton, TweenInfo.new(0.2, Enum.EasingStyle.Quad), {
			BackgroundTransparency = 0.05
		}):Play()
	end)

	MinimizeButton.MouseLeave:Connect(function()
		tweenService:Create(MinimizeButton, TweenInfo.new(0.2, Enum.EasingStyle.Quad), {
			BackgroundTransparency = 0.2
		}):Play()
	end)
	
	-- Hover effects for minimizer box
	MinimizerButton.MouseEnter:Connect(function()
		tweenService:Create(MinimizerBox, TweenInfo.new(0.2, Enum.EasingStyle.Quad), {
			BackgroundTransparency = 0.1,
			Size = UDim2.new(0, 55, 0, 55)
		}):Play()
		tweenService:Create(MinimizerStroke, TweenInfo.new(0.2, Enum.EasingStyle.Quad), {
			Transparency = 0.1
		}):Play()
	end)

	MinimizerButton.MouseLeave:Connect(function()
		tweenService:Create(MinimizerBox, TweenInfo.new(0.2, Enum.EasingStyle.Quad), {
			BackgroundTransparency = 0.3,
			Size = UDim2.new(0, 50, 0, 50)
		}):Play()
		tweenService:Create(MinimizerStroke, TweenInfo.new(0.2, Enum.EasingStyle.Quad), {
			Transparency = 0.3
		}):Play()
	end)
	
	-- ========== TOGGLE UI FUNCTION ==========
	function Lib:ToggleUI()
		if UiLib.Enabled then
			local ClickSound = Instance.new("Sound")
			ClickSound.Parent = UiLib
			ClickSound.SoundId = "rbxassetid://552900451"
			ClickSound.Volume = 2 
			ClickSound.Pitch = 1.2
			UiLib.Enabled = false
			ClickSound:Play()
			wait(0.5)
			ClickSound:Destroy()
		else
			local ClickSound = Instance.new("Sound")
			ClickSound.Parent = UiLib
			ClickSound.SoundId = "rbxassetid://552900451"
			ClickSound.Volume = 2 
			ClickSound.Pitch = 1
			UiLib.Enabled = true
			ClickSound:Play()
			wait(0.5)
			ClickSound:Destroy()
		end
	end
	
	local TabSys = {}
	local first = true

	-- ========== CREATE TAB ==========
	function TabSys.CreateTab(TabTitle)
		TabTitle = TabTitle or "Home"

		local TabContent = Instance.new("ScrollingFrame")
		local TabSwitcher = Instance.new("TextButton")
		local TabCorner = Instance.new("UICorner")
		local TabStroke = Instance.new("UIStroke")
		local ContentPadding = Instance.new("UIPadding")
		local ContentLayout = Instance.new("UIListLayout")
		local TabIcon = Instance.new("Frame")

		-- ========== TAB BUTTON ==========
		TabSwitcher.Name = "TabSwitcher"
		TabSwitcher.Parent = TabNavigator
		TabSwitcher.BackgroundColor3 = Color3.fromRGB(20, 35, 60)
		TabSwitcher.BackgroundTransparency = 0.3
		TabSwitcher.BorderSizePixel = 0
		TabSwitcher.Size = UDim2.new(0, 114, 0, 28)
		TabSwitcher.ZIndex = 4
		TabSwitcher.Font = Enum.Font.GothamSemibold
		TabSwitcher.Text = TabTitle
		TabSwitcher.TextColor3 = Color3.fromRGB(200, 220, 255)
		TabSwitcher.TextSize = 16
		TabSwitcher.TextWrapped = true

		TabCorner.CornerRadius = UDim.new(0, 8)
		TabCorner.Parent = TabSwitcher

		TabStroke.Parent = TabSwitcher
		TabStroke.Color = Color3.fromRGB(50, 130, 255)
		TabStroke.Thickness = 1.5
		TabStroke.Transparency = 0.7

		TabIcon.Name = "TabIcon"
		TabIcon.Parent = TabSwitcher
		TabIcon.BackgroundColor3 = Color3.fromRGB(60, 150, 255)
		TabIcon.BorderSizePixel = 0
		TabIcon.Position = UDim2.new(0, 0, 0, 0)
		TabIcon.Size = UDim2.new(0, 3, 1, 0)
		TabIcon.ZIndex = 5
		TabIcon.BackgroundTransparency = 1

		-- ========== TAB CONTENT (FIXED SCROLLING) ==========
		TabContent.Name = TabTitle.."'s Content"
		TabContent.Parent = ContentHolder
		TabContent.Active = true
		TabContent.BackgroundTransparency = 1
		TabContent.BorderSizePixel = 0
		TabContent.Position = UDim2.new(0.331549704, 0, 0.153153151, 0)
		TabContent.Size = UDim2.new(0, 295, 0, 275)
		TabContent.ZIndex = 4
		TabContent.ClipsDescendants = true
		TabContent.CanvasSize = UDim2.new(0, 0, 0, 0)
		TabContent.ScrollBarThickness = 4
		TabContent.ScrollBarImageColor3 = Color3.fromRGB(60, 150, 255)
		TabContent.AutomaticCanvasSize = Enum.AutomaticSize.Y

		ContentPadding.Parent = TabContent
		ContentPadding.PaddingTop = UDim.new(0, 12)
		ContentPadding.PaddingBottom = UDim.new(0, 12)

		ContentLayout.Parent = TabContent
		ContentLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
		ContentLayout.SortOrder = Enum.SortOrder.LayoutOrder
		ContentLayout.Padding = UDim.new(0, 10)

		-- First tab is visible
		if first then
			first = false
			TabContent.Visible = true
			TabSwitcher.BackgroundTransparency = 0.05
			TabSwitcher.TextColor3 = Color3.fromRGB(255, 255, 255)
			TabStroke.Transparency = 0.2
			TabIcon.BackgroundTransparency = 0
		else
			TabContent.Visible = false
			TabSwitcher.BackgroundTransparency = 0.5
			TabSwitcher.TextColor3 = Color3.fromRGB(150, 170, 200)
			TabStroke.Transparency = 0.8
			TabIcon.BackgroundTransparency = 1
		end

		-- Tab switching
		TabSwitcher.MouseButton1Click:Connect(function()
			for i,v in next, ContentHolder:GetChildren() do
				v.Visible = false
			end
			TabContent.Visible = true

			for i,v in next, TabNavigator:GetChildren() do
				if v:IsA("TextButton") then
					tweenService:Create(v, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
						BackgroundTransparency = 0.5
					}):Play()
					tweenService:Create(v, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
						TextColor3 = Color3.fromRGB(150, 170, 200)
					}):Play()
					if v:FindFirstChild("TabStroke") then
						tweenService:Create(v.TabStroke, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
							Transparency = 0.8
						}):Play()
					end
					if v:FindFirstChild("TabIcon") then
						tweenService:Create(v.TabIcon, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
							BackgroundTransparency = 1
						}):Play()
					end
				end
			end
			tweenService:Create(TabSwitcher, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
				BackgroundTransparency = 0.05
			}):Play()
			tweenService:Create(TabSwitcher, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
				TextColor3 = Color3.fromRGB(255, 255, 255)
			}):Play()
			tweenService:Create(TabStroke, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
				Transparency = 0.2
			}):Play()
			tweenService:Create(TabIcon, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
				BackgroundTransparency = 0
			}):Play()
		end)

		local Content = {}

		-- ========== CREATE BUTTON ==========
		function Content.CreateButton(BtnTitle, callback)
			BtnTitle = BtnTitle or "Button"
			callback = callback or function() end

			local ButtonFrame = Instance.new("Frame")	
			local ButtonTrigger = Instance.new("TextButton")
			local ButtonCorner = Instance.new("UICorner")
			local ButtonStroke = Instance.new("UIStroke")
			local TextLabel = Instance.new("TextLabel")
			local ButtonAccent = Instance.new("Frame")

			ButtonFrame.Name = "ButtonFrame"
			ButtonFrame.Parent = TabContent
			ButtonFrame.Active = true
			ButtonFrame.BackgroundColor3 = Color3.fromRGB(18, 30, 50)
			ButtonFrame.BackgroundTransparency = 0.15
			ButtonFrame.BorderSizePixel = 0
			ButtonFrame.ClipsDescendants = true
			ButtonFrame.Size = UDim2.new(0, 267, 0, 38)
			ButtonFrame.ZIndex = 4

			ButtonCorner.CornerRadius = UDim.new(0, 10)
			ButtonCorner.Parent = ButtonFrame

			ButtonStroke.Parent = ButtonFrame
			ButtonStroke.Color = Color3.fromRGB(55, 140, 255)
			ButtonStroke.Thickness = 1.5
			ButtonStroke.Transparency = 0.5

			ButtonAccent.Name = "ButtonAccent"
			ButtonAccent.Parent = ButtonFrame
			ButtonAccent.BackgroundColor3 = Color3.fromRGB(60, 150, 255)
			ButtonAccent.BackgroundTransparency = 0.7
			ButtonAccent.BorderSizePixel = 0
			ButtonAccent.Size = UDim2.new(0, 3, 1, 0)
			ButtonAccent.ZIndex = 5

			ButtonTrigger.Name = "ButtonTrigger"
			ButtonTrigger.Parent = ButtonFrame
			ButtonTrigger.BackgroundTransparency = 1
			ButtonTrigger.BorderSizePixel = 0
			ButtonTrigger.Size = UDim2.new(1, 0, 1, 0)
			ButtonTrigger.ZIndex = 5
			ButtonTrigger.Text = ""

			TextLabel.Parent = ButtonFrame
			TextLabel.BackgroundTransparency = 1
			TextLabel.BorderSizePixel = 0
			TextLabel.Position = UDim2.new(0.06, 0, 0, 0)
			TextLabel.Size = UDim2.new(0.88, 0, 1, 0)
			TextLabel.ZIndex = 5
			TextLabel.Font = Enum.Font.Gotham
			TextLabel.Text = BtnTitle
			TextLabel.TextColor3 = Color3.fromRGB(230, 240, 255)
			TextLabel.TextSize = 15
			TextLabel.TextXAlignment = Enum.TextXAlignment.Center

			ButtonTrigger.MouseEnter:Connect(function()
				tweenService:Create(ButtonFrame, TweenInfo.new(0.2, Enum.EasingStyle.Quad), {
					BackgroundTransparency = 0.05
				}):Play()
				tweenService:Create(ButtonStroke, TweenInfo.new(0.2, Enum.EasingStyle.Quad), {
					Transparency = 0.2
				}):Play()
				tweenService:Create(ButtonAccent, TweenInfo.new(0.2, Enum.EasingStyle.Quad), {
					BackgroundTransparency = 0.4
				}):Play()
			end)

			ButtonTrigger.MouseLeave:Connect(function()
				tweenService:Create(ButtonFrame, TweenInfo.new(0.2, Enum.EasingStyle.Quad), {
					BackgroundTransparency = 0.15
				}):Play()
				tweenService:Create(ButtonStroke, TweenInfo.new(0.2, Enum.EasingStyle.Quad), {
					Transparency = 0.5
				}):Play()
				tweenService:Create(ButtonAccent, TweenInfo.new(0.2, Enum.EasingStyle.Quad), {
					BackgroundTransparency = 0.7
				}):Play()
			end)

			ButtonTrigger.MouseButton1Click:Connect(function()
				local ClickSound = Instance.new("Sound")
				ClickSound.Parent = ButtonTrigger
				ClickSound.SoundId = "rbxassetid://552900451"
				ClickSound.Volume = 2 
				ClickSound.Pitch = 0.5
				ClickSound:Play()
				callback()
			end)
		end

		-- ========== CREATE SLIDER ==========
		function Content.CreateSlider(SliderTitle, minvalue, maxvalue, callback)
			SliderTitle = SliderTitle or "Slider"
			minvalue = minvalue or 16
			maxvalue = maxvalue or 100
			callback = callback or function() end

			local SliderFrame = Instance.new("Frame")
			local SliderCorner = Instance.new("UICorner")
			local SliderStroke = Instance.new("UIStroke")
			local SliderLabel = Instance.new("TextLabel")
			local SliderTrigger = Instance.new("TextButton")
			local SliderBack = Instance.new("Frame")
			local SliderFill = Instance.new("Frame")
			local UICorner_2 = Instance.new("UICorner")
			local UICorner_3 = Instance.new("UICorner")
			local ValueLabel = Instance.new("TextLabel")

			SliderFrame.Name = "SliderFrame"
			SliderFrame.Parent = TabContent
			SliderFrame.Active = true
			SliderFrame.BackgroundColor3 = Color3.fromRGB(18, 30, 50)
			SliderFrame.BackgroundTransparency = 0.15
