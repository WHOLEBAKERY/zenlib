			SliderStroke.Parent = SliderFrame
			SliderStroke.Color = Color3.fromRGB(55, 140, 255)
			SliderStroke.Thickness = 1.5
			SliderStroke.Transparency = 0.5

			SliderLabel.Parent = SliderFrame
			SliderLabel.BackgroundTransparency = 1
			SliderLabel.Position = UDim2.new(0.05, 0, 0.05, 0)
			SliderLabel.Size = UDim2.new(0, 160, 0, 25)
			SliderLabel.ZIndex = 5
			SliderLabel.Font = Enum.Font.GothamMedium
			SliderLabel.Text = SliderTitle
			SliderLabel.TextColor3 = Color3.fromRGB(220, 235, 255)
			SliderLabel.TextSize = 16
			SliderLabel.TextXAlignment = Enum.TextXAlignment.Left

			SliderTrigger.Parent = SliderFrame
			SliderTrigger.BackgroundTransparency = 1
			SliderTrigger.Position = UDim2.new(0.065, 0, 0.65, 0)
			SliderTrigger.Size = UDim2.new(0, 230, 0, 8)
			SliderTrigger.ZIndex = 5
			SliderTrigger.Text = ""

			SliderBack.Parent = SliderTrigger
			SliderBack.BackgroundColor3 = Color3.fromRGB(15, 25, 40)
			SliderBack.BorderSizePixel = 0
			SliderBack.Size = UDim2.new(1, 0, 1, 0)
			SliderBack.ZIndex = 5
			SliderBack.ClipsDescendants = true
			
			SliderFill.Parent = SliderBack
			SliderFill.BackgroundColor3 = Color3.fromRGB(50, 130, 255)
			SliderFill.BorderSizePixel = 0
			SliderFill.Size = UDim2.new(0, 0, 1, 0)
			SliderFill.ZIndex = 5

			UICorner_2.CornerRadius = UDim.new(1, 0)
			UICorner_2.Parent = SliderFill

			UICorner_3.CornerRadius = UDim.new(1, 0)
			UICorner_3.Parent = SliderBack

			ValueLabel.Parent = SliderFrame
			ValueLabel.BackgroundTransparency = 1
			ValueLabel.Position = UDim2.new(0.65, 0, 0.05, 0)
			ValueLabel.Size = UDim2.new(0, 80, 0, 25)
			ValueLabel.ZIndex = 5
			ValueLabel.Font = Enum.Font.GothamBold
			ValueLabel.Text = minvalue
			ValueLabel.TextColor3 = Color3.fromRGB(100, 180, 255)
			ValueLabel.TextSize = 16
			ValueLabel.TextTransparency = 1

			local mouse = game.Players.LocalPlayer:GetMouse()
			local uis = game:GetService("UserInputService")
			local Value

			SliderTrigger.MouseButton1Down:Connect(function()
				tweenService:Create(ValueLabel, TweenInfo.new(0.1, Enum.EasingStyle.Linear), {
					TextTransparency = 0
				}):Play()
				Value = math.floor((((tonumber(maxvalue) - tonumber(minvalue)) / 229) * SliderFill.AbsoluteSize.X) + tonumber(minvalue)) or 0
				pcall(function()
					callback(Value)
				end)
				SliderFill:TweenSize(UDim2.new(0, math.clamp(mouse.X - SliderFill.AbsolutePosition.X, 0, 229), 1, 0), "InOut", "Linear", 0.15, true)
				moveconnection = mouse.Move:Connect(function()
					ValueLabel.Text = Value
					Value = math.floor((((tonumber(maxvalue) - tonumber(minvalue)) / 229) * SliderFill.AbsoluteSize.X) + tonumber(minvalue))
					pcall(function()
						callback(Value)
					end)
					SliderFill:TweenSize(UDim2.new(0, math.clamp(mouse.X - SliderFill.AbsolutePosition.X, 0, 229), 1, 0), "InOut", "Linear", 0.15, true)
				end)
				releaseconnection = uis.InputEnded:Connect(function(Mouse)
					if Mouse.UserInputType == Enum.UserInputType.MouseButton1 then
						Value = math.floor((((tonumber(maxvalue) - tonumber(minvalue)) / 229) * SliderFill.AbsoluteSize.X) + tonumber(minvalue))
						pcall(function()
							callback(Value)
						end)
						ValueLabel.Text = Value
						tweenService:Create(ValueLabel, TweenInfo.new(0.1, Enum.EasingStyle.Linear), {
							TextTransparency = 1
						}):Play()
						SliderFill:TweenSize(UDim2.new(0, math.clamp(mouse.X - SliderFill.AbsolutePosition.X, 0, 229), 1, 0), "InOut", "Linear", 0.15, true)
						moveconnection:Disconnect()
						releaseconnection:Disconnect()
					end
				end)
			end)
		end
		
		-- ========== CREATE TOGGLE ==========
		function Content.CreateToggle(TogTXT, callback)
			TogTXT = TogTXT or "Toggle"
			callback = callback or function() end
			local toggled = false
			
			local ToggleFrame = Instance.new("Frame")
			local ButtonCorner = Instance.new("UICorner")
			local ToggleStroke = Instance.new("UIStroke")
			local ToggleLabel = Instance.new("TextLabel")
			local ToggleTrigger = Instance.new("TextButton")
			local ToggleButton = Instance.new("Frame")
			local ToggleButtonCorner = Instance.new("UICorner")
			local ToggleIndicator = Instance.new("Frame")
			local IndicatorCorner = Instance.new("UICorner")

			ToggleFrame.Name = "ToggleFrame"
			ToggleFrame.Parent = TabContent
			ToggleFrame.Active = true
			ToggleFrame.BackgroundColor3 = Color3.fromRGB(18, 30, 50)
			ToggleFrame.BackgroundTransparency = 0.15
			ToggleFrame.BorderSizePixel = 0
			ToggleFrame.Size = UDim2.new(0, 267, 0, 38)
			ToggleFrame.ZIndex = 4

			ButtonCorner.CornerRadius = UDim.new(0, 10)
			ButtonCorner.Parent = ToggleFrame

			ToggleStroke.Parent = ToggleFrame
			ToggleStroke.Color = Color3.fromRGB(55, 140, 255)
			ToggleStroke.Thickness = 1.5
			ToggleStroke.Transparency = 0.5

			ToggleLabel.Parent = ToggleFrame
			ToggleLabel.BackgroundTransparency = 1
			ToggleLabel.Position = UDim2.new(0.05, 0, 0, 0)
			ToggleLabel.Size = UDim2.new(0, 180, 0, 38)
			ToggleLabel.ZIndex = 5
			ToggleLabel.Font = Enum.Font.Gotham
			ToggleLabel.Text = TogTXT
			ToggleLabel.TextColor3 = Color3.fromRGB(220, 235, 255)
			ToggleLabel.TextSize = 16
			ToggleLabel.TextXAlignment = Enum.TextXAlignment.Left

			ToggleTrigger.Parent = ToggleFrame
			ToggleTrigger.BackgroundTransparency = 1
			ToggleTrigger.Size = UDim2.new(1, 0, 1, 0)
			ToggleTrigger.ZIndex = 5
			ToggleTrigger.Text = ""

			ToggleButton.Parent = ToggleFrame
			ToggleButton.BackgroundColor3 = Color3.fromRGB(25, 35, 55)
			ToggleButton.BorderSizePixel = 0
			ToggleButton.Position = UDim2.new(0.82, 0, 0.25, 0)
			ToggleButton.Size = UDim2.new(0, 40, 0, 19)
			ToggleButton.ZIndex = 5

			ToggleButtonCorner.CornerRadius = UDim.new(1, 0)
			ToggleButtonCorner.Parent = ToggleButton

			ToggleIndicator.Parent = ToggleButton
			ToggleIndicator.BackgroundColor3 = Color3.fromRGB(100, 110, 130)
			ToggleIndicator.BorderSizePixel = 0
			ToggleIndicator.Position = UDim2.new(0.08, 0, 0.1, 0)
			ToggleIndicator.Size = UDim2.new(0, 15, 0, 15)
			ToggleIndicator.ZIndex = 6

			IndicatorCorner.CornerRadius = UDim.new(1, 0)
			IndicatorCorner.Parent = ToggleIndicator
			
			ToggleTrigger.MouseButton1Click:Connect(function()
				if toggled == false then
					tweenService:Create(ToggleIndicator, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
						Position = UDim2.new(0.54, 0, 0.1, 0),
						BackgroundColor3 = Color3.fromRGB(100, 200, 255)
					}):Play()
					tweenService:Create(ToggleButton, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
						BackgroundColor3 = Color3.fromRGB(40, 100, 180)
					}):Play()
					
					local ClickSound = Instance.new("Sound")
					ClickSound.Parent = ToggleFrame
					ClickSound.SoundId = "rbxassetid://552900451"
					ClickSound.Volume = 2 
					ClickSound.Pitch = 1
					ClickSound:Play()
					wait(0.25)
					toggled = true
				elseif toggled == true then
					tweenService:Create(ToggleIndicator, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
						Position = UDim2.new(0.08, 0, 0.1, 0),
						BackgroundColor3 = Color3.fromRGB(100, 110, 130)
					}):Play()
					tweenService:Create(ToggleButton, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
						BackgroundColor3 = Color3.fromRGB(25, 35, 55)
					}):Play()

					local ClickSound = Instance.new("Sound")
					ClickSound.Parent = ToggleFrame
					ClickSound.SoundId = "rbxassetid://552900451"
					ClickSound.Volume = 2 
					ClickSound.Pitch = 1.2
					ClickSound:Play()
					wait(0.25)
					toggled = false
				end
				pcall(callback, toggled)
			end)
		end
		
		-- ========== CREATE KEYBIND ==========
		function Content.CreateKeybind(KeybindTitle, first, callback)
			KeybindTitle = KeybindTitle or "Keybind"
			local oldKey = first.Name
			callback = callback or function() end

			local KeybindFrame = Instance.new("Frame")
			local KeybindCorner = Instance.new("UICorner")
			local KeybindStroke = Instance.new("UIStroke")
			local KeybindLabel = Instance.new("TextLabel")
			local Input = Instance.new("TextLabel")
			local ToggleTrigger = Instance.new("TextButton")
			local KeybindCorner_2 = Instance.new("UICorner")

			KeybindFrame.Name = "KeybindFrame"
			KeybindFrame.Parent = TabContent
			KeybindFrame.Active = true
			KeybindFrame.BackgroundColor3 = Color3.fromRGB(18, 30, 50)
			KeybindFrame.BackgroundTransparency = 0.15
			KeybindFrame.BorderSizePixel = 0
			KeybindFrame.Size = UDim2.new(0, 267, 0, 38)
			KeybindFrame.ZIndex = 4

			KeybindCorner.CornerRadius = UDim.new(0, 10)
			KeybindCorner.Parent = KeybindFrame

			KeybindStroke.Parent = KeybindFrame
			KeybindStroke.Color = Color3.fromRGB(55, 140, 255)
			KeybindStroke.Thickness = 1.5
			KeybindStroke.Transparency = 0.5

			KeybindLabel.Parent = KeybindFrame
			KeybindLabel.BackgroundTransparency = 1
			KeybindLabel.Position = UDim2.new(0.05, 0, 0, 0)
			KeybindLabel.Size = UDim2.new(0, 150, 0, 38)
			KeybindLabel.ZIndex = 5
			KeybindLabel.Font = Enum.Font.GothamMedium
			KeybindLabel.Text = KeybindTitle
			KeybindLabel.TextColor3 = Color3.fromRGB(220, 235, 255)
			KeybindLabel.TextSize = 16
			KeybindLabel.TextXAlignment = Enum.TextXAlignment.Left

			Input.Parent = KeybindFrame
			Input.BackgroundColor3 = Color3.fromRGB(25, 40, 70)
			Input.BorderSizePixel = 0
			Input.Position = UDim2.new(0.75, 0, 0.2, 0)
			Input.Size = UDim2.new(0, 55, 0, 23)
			Input.ZIndex = 5
			Input.Font = Enum.Font.GothamBold
			Input.Text = oldKey
			Input.TextColor3 = Color3.fromRGB(100, 180, 255)
			Input.TextScaled = true

			KeybindCorner_2.CornerRadius = UDim.new(0, 6)
			KeybindCorner_2.Parent = Input

			ToggleTrigger.Parent = KeybindFrame
			ToggleTrigger.BackgroundTransparency = 1
			ToggleTrigger.Size = UDim2.new(1, 0, 1, 0)
			ToggleTrigger.ZIndex = 5
			ToggleTrigger.Text = ""

			ToggleTrigger.MouseButton1Click:Connect(function()
				Input.Text = "..."
				tweenService:Create(Input, TweenInfo.new(0.2), {
					BackgroundColor3 = Color3.fromRGB(35, 60, 100)
				}):Play()
				local a, b = game:GetService('UserInputService').InputBegan:wait()
				if a.KeyCode.Name ~= "Unknown" then
					Input.Text = a.KeyCode.Name
					oldKey = a.KeyCode.Name
				end
				tweenService:Create(Input, TweenInfo.new(0.2), {
					BackgroundColor3 = Color3.fromRGB(25, 40, 70)
				}):Play()
			end)

			game:GetService("UserInputService").InputBegan:connect(function(current, ok) 
				if not ok then 
					if current.KeyCode.Name == oldKey then 
						callback()
					end
				end
			end)
		end
		
		-- ========== CREATE TEXTBOX ==========
		function Content.CreateTextbox(TextboxTitle, callback)
			TextboxTitle = TextboxTitle or "Textbox"
			callback = callback or function() end
			
			local TextboxFrame = Instance.new("Frame")
			local TextboxCorner = Instance.new("UICorner")
			local TextboxStroke = Instance.new("UIStroke")
			local TextboxLabel = Instance.new("TextLabel")
			local Input = Instance.new("TextBox")
			local KeybindCorner = Instance.new("UICorner")

			TextboxFrame.Name = "TextboxFrame"
			TextboxFrame.Parent = TabContent
			TextboxFrame.Active = true
			TextboxFrame.BackgroundColor3 = Color3.fromRGB(18, 30, 50)
			TextboxFrame.BackgroundTransparency = 0.15
			TextboxFrame.BorderSizePixel = 0
			TextboxFrame.Size = UDim2.new(0, 267, 0, 60)
			TextboxFrame.ZIndex = 4

			TextboxCorner.CornerRadius = UDim.new(0, 10)
			TextboxCorner.Parent = TextboxFrame

			TextboxStroke.Parent = TextboxFrame
			TextboxStroke.Color = Color3.fromRGB(55, 140, 255)
			TextboxStroke.Thickness = 1.5
			TextboxStroke.Transparency = 0.5

			TextboxLabel.Parent = TextboxFrame
			TextboxLabel.BackgroundTransparency = 1
			TextboxLabel.Position = UDim2.new(0.05, 0, 0.08, 0)
			TextboxLabel.Size = UDim2.new(0, 240, 0, 22)
			TextboxLabel.ZIndex = 5
			TextboxLabel.Font = Enum.Font.GothamMedium
			TextboxLabel.Text = TextboxTitle
			TextboxLabel.TextColor3 = Color3.fromRGB(220, 235, 255)
			TextboxLabel.TextSize = 16
			TextboxLabel.TextXAlignment = Enum.TextXAlignment.Left

			Input.Parent = TextboxFrame
			Input.BackgroundColor3 = Color3.fromRGB(12, 20, 35)
			Input.BorderSizePixel = 0
			Input.Position = UDim2.new(0.08, 0, 0.55, 0)
			Input.Size = UDim2.new(0, 225, 0, 22)
			Input.ZIndex = 5
			Input.Font = Enum.Font.Gotham
			Input.PlaceholderText = "Type here..."
			Input.PlaceholderColor3 = Color3.fromRGB(100, 120, 150)
			Input.Text = ""
			Input.TextColor3 = Color3.fromRGB(200, 220, 255)
			Input.TextScaled = true
			Input.TextSize = 14

			KeybindCorner.CornerRadius = UDim.new(0, 6)
			KeybindCorner.Parent = Input
			
			Input.Focused:Connect(function()
				tweenService:Create(Input, TweenInfo.new(0.2), {
					BackgroundColor3 = Color3.fromRGB(18, 30, 50)
				}):Play()
			end)
			
			Input.FocusLost:Connect(function(EnterPressed)
				tweenService:Create(Input, TweenInfo.new(0.2), {
					BackgroundColor3 = Color3.fromRGB(12, 20, 35)
				}):Play()
				if not EnterPressed then 
					return
				else
					callback(Input.Text)
					wait(0.18)
					Input.Text = ""  
				end
			end)
		end
		
		-- ========== CREATE DROPDOWN ==========
		function Content.CreateDropdown(DropdownTitle, list, callback)
			callback = callback or function() end
			DropdownTitle = DropdownTitle or "Dropdown"
			list = list or {}
			local IsDropped = false
			local DropYSize = 145
			local NormalYSize = 38
			
			local DropdownFrame = Instance.new("Frame")
			local DropdownCorner = Instance.new("UICorner")
			local DropdownStroke = Instance.new("UIStroke")
			local DropdownLabel = Instance.new("TextLabel")		
			local DropdownTrigger = Instance.new("TextButton")
			local DropArrow = Instance.new("TextLabel")
			local DropdownContainer = Instance.new("ScrollingFrame")
			local ContentLayout = Instance.new("UIListLayout")
			local ContentPadding = Instance.new("UIPadding")

			DropdownFrame.Name = "DropdownFrame"
			DropdownFrame.Parent = TabContent
			DropdownFrame.Active = true
			DropdownFrame.BackgroundColor3 = Color3.fromRGB(18, 30, 50)
			DropdownFrame.BackgroundTransparency = 0.15
			DropdownFrame.BorderSizePixel = 0
			DropdownFrame.ClipsDescendants = true
			DropdownFrame.Size = UDim2.new(0, 267, 0, NormalYSize)
			DropdownFrame.ZIndex = 4

			DropdownCorner.CornerRadius = UDim.new(0, 10)
			DropdownCorner.Parent = DropdownFrame

			DropdownStroke.Parent = DropdownFrame
			DropdownStroke.Color = Color3.fromRGB(55, 140, 255)
			DropdownStroke.Thickness = 1.5
			DropdownStroke.Transparency = 0.5

			DropdownLabel.Parent = DropdownFrame
			DropdownLabel.BackgroundTransparency = 1
			DropdownLabel.Position = UDim2.new(0.05, 0, 0, 0)
			DropdownLabel.Size = UDim2.new(0, 200, 0, 38)
			DropdownLabel.ZIndex = 5
			DropdownLabel.Font = Enum.Font.Gotham
			DropdownLabel.Text = DropdownTitle
			DropdownLabel.TextColor3 = Color3.fromRGB(220, 235, 255)
			DropdownLabel.TextSize = 16
			DropdownLabel.TextXAlignment = Enum.TextXAlignment.Left
	
			DropArrow.Parent = DropdownFrame
			DropArrow.BackgroundTransparency = 1
			DropArrow.Position = UDim2.new(0.88, 0, 0.26, 0)
			DropArrow.Size = UDim2.new(0, 20, 0, 20)
			DropArrow.ZIndex = 5
			DropArrow.Font = Enum.Font.GothamBold
			DropArrow.Text = "▼"
			DropArrow.TextColor3 = Color3.fromRGB(100, 180, 255)
			DropArrow.TextSize = 12
			
			DropdownTrigger.Parent = DropdownFrame
			DropdownTrigger.BackgroundTransparency = 1
			DropdownTrigger.Size = UDim2.new(1, 0, 0, 38)
			DropdownTrigger.ZIndex = 5
			DropdownTrigger.Text = ""
			
			DropdownTrigger.MouseButton1Click:Connect(function()
				if IsDropped == false then 
					local ClickSound = Instance.new("Sound")
					ClickSound.Parent = UiLib
					ClickSound.SoundId = "rbxassetid://552900451"
					ClickSound.Volume = 2 
					ClickSound.Pitch = 1.2
					
					tweenService:Create(DropArrow, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
						Rotation = 180
					}):Play()
					
					tweenService:Create(DropdownFrame, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
						Size = UDim2.new(0, 267, 0, DropYSize)
					}):Play()
					DropdownContainer.Visible = true
					ClickSound:Play()
					IsDropped = true
				else
					local ClickSound = Instance.new("Sound")
					ClickSound.Parent = UiLib
					ClickSound.SoundId = "rbxassetid://552900451"
					ClickSound.Volume = 2 
					ClickSound.Pitch = 1

					tweenService:Create(DropArrow, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
						Rotation = 0
					}):Play()

					tweenService:Create(DropdownFrame, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
						Size = UDim2.new(0, 267, 0, NormalYSize)
					}):Play()
					DropdownContainer.Visible = false
					ClickSound:Play()
					IsDropped = false
				end
			end)

			DropdownContainer.Parent = DropdownFrame
			DropdownContainer.Active = true
			DropdownContainer.BackgroundTransparency = 1
			DropdownContainer.BorderSizePixel = 0
			DropdownContainer.Position = UDim2.new(0.06, 0, 0.32, 0)
			DropdownContainer.Size = UDim2.new(0, 237, 0, 100)
			DropdownContainer.ZIndex = 5
			DropdownContainer.CanvasSize = UDim2.new(0, 0, 0, 0)
			DropdownContainer.Visible = false
			DropdownContainer.ScrollBarThickness = 4
			DropdownContainer.ScrollBarImageColor3 = Color3.fromRGB(60, 150, 255)
			DropdownContainer.AutomaticCanvasSize = Enum.AutomaticSize.Y

			ContentLayout.Parent = DropdownContainer
			ContentLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
			ContentLayout.SortOrder = Enum.SortOrder.LayoutOrder
			ContentLayout.Padding = UDim.new(0, 6)
			
			for i,v in next, list do
				local Option = Instance.new("TextButton")
				local UICorner = Instance.new("UICorner")
				
				Option.Name = "Option"
				Option.Parent = DropdownContainer
				Option.BackgroundColor3 = Color3.fromRGB(25, 40, 70)
				Option.BackgroundTransparency = 0.2
				Option.BorderSizePixel = 0
				Option.Size = UDim2.new(0, 220, 0, 26)
				Option.Font = Enum.Font.Gotham
				Option.Text = v
				Option.TextColor3 = Color3.fromRGB(200, 220, 255)
				Option.TextSize = 14
				Option.MouseButton1Click:Connect(function()
					local ClickSound = Instance.new("Sound")
					ClickSound.Parent = UiLib
					ClickSound.SoundId = "rbxassetid://552900451"
					ClickSound.Volume = 2 
					ClickSound.Pitch = 1

					callback(v)
					ClickSound:Play()
					
					tweenService:Create(DropArrow, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
						Rotation = 0
					}):Play()
					
					tweenService:Create(DropdownFrame, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
						Size = UDim2.new(0, 267, 0, NormalYSize)
					}):Play()
					DropdownContainer.Visible = false		
					IsDropped = false

					wait(0.5)
					ClickSound:Destroy()
				end)

				Option.MouseEnter:Connect(function()
					tweenService:Create(Option, TweenInfo.new(0.2, Enum.EasingStyle.Quad), {
						BackgroundTransparency = 0.05
					}):Play()
				end)
				
				Option.MouseLeave:Connect(function()
					tweenService:Create(Option, TweenInfo.new(0.2, Enum.EasingStyle.Quad), {
						BackgroundTransparency = 0.2
					}):Play()
				end)

				UICorner.CornerRadius = UDim.new(0, 6)
				UICorner.Parent = Option
			end

			ContentPadding.Parent = DropdownContainer
			ContentPadding.PaddingTop = UDim.new(0, 8)
			ContentPadding.PaddingBottom = UDim.new(0, 8)
		end
		
