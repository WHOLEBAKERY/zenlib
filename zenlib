	-- ========== TOGGLE UI FUNCTION ==========
	function Lib:ToggleUI()
		if UiLib.Enabled then
			local ClickSound = Instance.new("Sound")
			ClickSound.Parent = UiLib
			ClickSound.SoundId = "rbxassetid://552900451"
			ClickSound.Volume = 2 
			ClickSound.Pitch = 1.2
			UiLib.Enabled = false
			ClickSound:Play()
			wait(0.5)
			ClickSound:Destroy()
		else
			local ClickSound = Instance.new("Sound")
			ClickSound.Parent = UiLib
			ClickSound.SoundId = "rbxassetid://552900451"
			ClickSound.Volume = 2 
			ClickSound.Pitch = 1
			UiLib.Enabled = true
			ClickSound:Play()
			wait(0.5)
			ClickSound:Destroy()
		end
	end
	
